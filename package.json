{
  "name": "async-job-queue",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run start:server && bun run start:worker",
    "dev": "bun run dev:server & bun run dev:worker",
    "start:worker": "bun run src/worker.ts",
    "dev:worker": "bun run --watch src/worker.ts",
    "start:server": "bun run src/index.ts",
    "dev:server": "bun run --watch src/index.ts",
    "format": "biome format --write .",
    "format:check": "biome format --check .",
    "lint": "biome lint .",
    "check": "biome check --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@bull-board/api": "^6.16.4",
    "@bull-board/fastify": "^6.16.4",
    "@bull-board/ui": "^6.16.4",
    "bullmq": "^5.67.1",
    "fastify": "^5.7.1",
    "fastify-type-provider-zod": "^6.1.0",
    "ioredis": "^5.9.2",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "sharp": "^0.34.5"
  }
}
